<style>
    body {
        font-family: "Jua", sans-serif;
    }

    @media (min-width: 0px) {
        .my_mobile_disappear {
            display: none;
        }
        .my_mobile_appear {
            display: block;
        }

        .my_chat_icon {
            position: fixed;
            width: 10%;
            padding-top: 10%;
            right: 5%;
            bottom: 5%;
            background-color: #f1f1f1;
            background-image: url("/images/chat.png");
            background-size: cover;
            resize: both;
        }
    }

    @media (min-width: 992px) {
        .my_mobile_disappear {
            display: block;
        }
        .my_mobile_appear {
            display: none;
        }

        .my_chat_icon {
            position: fixed;
            width: 5%;
            padding-top: 5%;
            right: 2%;
            bottom: 2%;
            background-color: #f1f1f1;
            background-image: url("/images/chat.png");
            background-size: cover;
            resize: both;
        }
    }
